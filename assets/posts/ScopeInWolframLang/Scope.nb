Cell[BoxData[{
 RowBox[{
  RowBox[{"BeginPackage", "[", "\"\<Collatz`\>\"", "]"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Collatz", "::", "usage"}], " ", "=", "\n", "        ", 
   "\"\<Collatz[n] gives a list of the iterates in the 3n+1 problem,\n        \
starting from n. The conjecture is that this sequence always\n        \
terminates.\>\""}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"Begin", "[", "\"\<`Private`\>\"", "]"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Collatz", "[", "1", "]"}], " ", ":=", " ", 
   RowBox[{"{", "1", "}"}]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Collatz", "[", "n_Integer", "]"}], "  ", ":=", " ", 
   RowBox[{
    RowBox[{"Prepend", "[", 
     RowBox[{
      RowBox[{"Collatz", "[", 
       RowBox[{
        RowBox[{"3", " ", "n"}], " ", "+", " ", "1"}], "]"}], ",", " ", "n"}],
      "]"}], " ", "/;", " ", 
    RowBox[{
     RowBox[{"OddQ", "[", "n", "]"}], " ", "&&", " ", 
     RowBox[{"n", " ", ">", " ", "0"}]}]}]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Collatz", "[", "n_Integer", "]"}], " ", ":=", " ", 
   RowBox[{
    RowBox[{"Prepend", "[", 
     RowBox[{
      RowBox[{"Collatz", "[", 
       RowBox[{"n", "/", "2"}], "]"}], ",", " ", "n"}], "]"}], " ", "/;", " ", 
    RowBox[{
     RowBox[{"EvenQ", "[", "n", "]"}], " ", "&&", " ", 
     RowBox[{"n", " ", ">", " ", "0"}]}]}]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"End", "[", " ", "]"}], "\n"}], "\n", 
 RowBox[{"EndPackage", "[", " ", "]"}]}], "Code",
